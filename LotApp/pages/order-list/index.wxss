.container{
  min-height: 100%;
  overflow: hidden;
  overflow-y: hidden;
}
.userinfo {
	display: flex;
	flex-direction: column;
	padding: 50rpx 0;
	align-items: center;
	background: #34B5E3;
  width: 750rpx;
}

.userinfo-avatar {
	width: 160rpx;
	height: 160rpx;
	margin: 20rpx;
	border-radius: 50%;
}

.userinfo-nickname {
  margin-top: 20rpx;
  color:  #FFFFFF;
}


.version {
  width:100%;
  height: 40rpx;
	line-height: 40rpx;
	font-size: 24rpx;
	text-align: center;  
  padding: 40rpx;
}

.my-item{
    width: 100%;
    background-size: 16rpx auto,750rpx auto;
    margin: 20rpx 0; 
    border-bottom: 1px solid #eee;
    line-height: 100rpx;
    height: 200rpx;
}

.item{
   text-align:left;
}

.frontStyle{
  font-size: 25px;
}

.wenben {
  flex-direction: row;
  justify-content: space-between;
  display: flex;
  /*flex-direction: row;默认值为row，所以可以被注释*/
  align-items: center;
}

.img {
    width:100rpx;
    height:100rpx;
    padding-left: 50rpx;
}

.left{
  font-family: "黑体";
  color: #666;
  font-size: 35rpx;
  padding-left: 50rpx;
  width: 100%;
}

.right {
  font-family: "黑体";
  color: #666;
  font-size: 35rpx;
  width: 100%;
}

.small
{
  font-family: "黑体";
  color: #666;
  font-size: 24rpx;
  padding-left: 50rpx;
  width: 100%;
}
